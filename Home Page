<script>
$(document).ready(function(){
    /* add the active class to the parent of the current li */
    $('.dropdown a').filter(function(){
        return $(this).attr('href') == location.pathname
    }).addClass('active').closest('ul').parent().addClass('active');


    /* default settings */
    $('.venobox').venobox();


    /* custom settings */
    $('.venobox_custom').venobox({
        framewidth: '400px',        // default: ''
        frameheight: '300px',       // default: ''
        border: '10px',             // default: '0'
        bgcolor: '#5dff5e',         // default: '#fff'
        titleattr: 'data-title',    // default: 'title'
        numeratio: true,            // default: false
        infinigall: true            // default: false
    });

    /* auto-open #firstlink on page load */
    $("#firstlink").venobox().trigger('click');


    var browser_language = window.navigator.userLanguage || window.navigator.language;
    //alert(browser_language.substring(0,3));
    if (browser_language.substring(0,2) == "en")
    {
        // Browser Language is English
        //alert('Browser Language is English: ' + browser_language);
        $('#drop-down-bar').text('Browser Language is English: ' + browser_language);
        //$("#drop-down-bar").show();
        $("#drop-down-bar").hide();
        //document.getElementById('drop-down-bar').style.display = 'inline';
    }
    else
    {
        // Browser Language is not English
        //alert('Browser Language is not English: ' + browser_language);
        $('#drop-down-bar').text('Browser Language is not English: ' + browser_language);
        //$("#drop-down-bar").show();
        $("#drop-down-bar").hide();
        //document.getElementById('drop-down-bar').style.display = 'inline';
    }
});
</script>

